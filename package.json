{
  "name": "art101-sales-scraper",
  "version": "1.0.0",
  "engines": {
    "node": "16.x"
  },
  "description": "",
  "main": "src/main.js",
  "type": "module",
  "scripts": {
    "serve": "node --max_old_space_size=128 --optimize_for_size src/server.js",
    "worker-main": "node --max_old_space_size=1024 --optimize_for_size src/main.js",
    "worker-events": "sleep 2 && node --max_old_space_size=1024 --optimize_for_size src/events.js",
    "start": "concurrently npm:worker* npm:serve --restart-tries -1 --restart-after 8000"
  },
  "author": "lza_menace",
  "license": "ISC",
  "dependencies": {
    "@types/util.promisify": "^1.0.4",
    "better-sqlite3": "^7.4.5",
    "bignumber.js": "^9.0.1",
    "concurrently": "^6.5.0",
    "dotenv": "^10.0.0",
    "express": "^4.17.1",
    "node-fetch": "^3.2.9",
    "sqlite3": "^5.0.2",
    "web3": "^1.6.1"
  }
}
